#include <stdio.h>
#include <stdlib.h>

int main()
{
    int n,i;
    printf("enter a number");
    scanf("%d",&n);
    for(i=2;i<n;i++)
    {
        if(n%i==0)
        {
          printf("NOT PRIME");
          break;
        }
        else
        {
            printf("PRIME");
            break;
        }
    }
    return 0;
}
